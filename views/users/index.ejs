<div class="container mt-5">
    <%- include('../part/message') %>
    <a type="button" class="btn btn-primary btn-lg" href="/user/register">Register new user</a>
    <h1>List User</h1>

    <table class="table table-dark table-hover">
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
        <%
        users.map(user=>{%>
            <tr>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <td><%= user.password %></td>
                <td>Edit</td>
                <td> <%- include('../part/deleteForm',{url:`/user/${user._id}?_method=DELETE`}) %></td>
            </tr>
        <%})
        %>
        
      
            
    </table>
</div>